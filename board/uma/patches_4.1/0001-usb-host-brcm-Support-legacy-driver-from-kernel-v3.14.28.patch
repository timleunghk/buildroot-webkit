+# Choose between legacy and new Broadcom STB USB drivers depending on config.
+ifneq ($(CONFIG_BRCM_USB_LEGACY), )
+	BRCM_USB_OBJS		:= usb-brcm-legacy.o usb-brcm-common-init.o
+	BRCM_USB_XHCI_OBJS	:= xhci-brcm-legacy.o
+	BRCM_USB_EHCI_OBJS	:= ehci-brcm-legacy.o
+	BRCM_USB_OHCI_OBJS	:= ohci-brcm-legacy.o
+else
+	BRCM_USB_OBJS		:= usb-brcm.o
+	BRCM_USB_XHCI_OBJS	:= xhci-brcm.o
+	BRCM_USB_EHCI_OBJS	:= ehci-brcm.o
+	BRCM_USB_OHCI_OBJS	:= ohci-brcm.o
+endif
+obj-$(CONFIG_BRCM_USB)		+= $(BRCM_USB_OBJS)
